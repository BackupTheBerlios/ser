             FROM SNMPv2-CONF    
       
        SnmpAdminString    
             FROM SNMP-FRAMEWORK-MIB    
  
        SipTransportProtocol   
             FROM SIP-TC   
    
        applIndex    
             FROM NETWORK-SERVICES-MIB;    
  
   sipCommonMIB MODULE-IDENTITY    
        LAST-UPDATED   "200202040000Z"    
        ORGANIZATION   "IETF SIP Working Group, SIP MIB Team"    
