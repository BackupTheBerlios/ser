# Stupid script for SER xml-rpc testing. 

#SER      = 'http://ser:heslo@213.192.30.152/'
SER      = 'https://ser:heslo@10.38.2.230/'
#SSL_KEY  = 'eve_prik.key'
#SSL_CERT = 'eve_cert.crt'

SSL_CERT = None
SSL_KEY  = None

from serctl.serxmlrpc import ServerProxy
import sys

if len(sys.argv) < 2:
	print "Usage: ser_rpc <command> [params...]"
	sys.exit()

ser = ServerProxy(SER, (SSL_KEY, SSL_CERT))

cmd = 'ser.' + sys.argv[1]
params = ', '.join(sys.argv[2:])

cmd = cmd + '(' + params + ')'

print cmd + ':'
print eval(cmd)
