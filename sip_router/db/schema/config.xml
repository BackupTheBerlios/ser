<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table PUBLIC "-//iptel.org//DTD DBSchema V1.0//EN"
  "http://iptel.org/dbschema/dtd/1.0/dbschema.dtd" [

<!ENTITY % entities SYSTEM "entities.xml">
%entities;

]>

<table xmlns:my="http://iptel.org/dbschema/mysql">
    <name>config</name>
    <version>1</version>

    <column>
	<name>attribute</name>
	<type>string</type>
	<size>32</size>
    </column>
    <column>
	<name>value</name>
	<type>string</type>
	<size>128</size>
    </column>
    <column>
	<name>username</name>
	<type>string</type>
	<default></default>
	<size>&user_len;</size>
    </column>
    <column>
	<name>domain</name>
	<type>string</type>
	<default></default>
	<size>&domain_len;</size>
    </column>
    <column>
	<name>modified</name>
	<type>datetime</type>
	<null/>
    </column>
</table>
