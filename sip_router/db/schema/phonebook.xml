	<default></default>
	<size>&user_len;</size>
    </column>
    <column>
	<name>domain</name>
	<type>string</type>
	<default></default>
	<size>&domain_len;</size>
    </column>
    <column>
	<name>fname</name>
	<type>string</type>
	<default></default>
	<size>32</size>
    </column>
    <column>
	<name>lname</name>
	<type>string</type>
	<default></default>
	<size>32</size>
    </column>
    <column>
	<name>sip_uri</name>
	<type>string</type>
	<default></default>
	<size>&uri_len;</size>
    </column>
    <index>
	<colref linkend="id"/>
    </index>
</table>
