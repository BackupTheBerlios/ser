<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table PUBLIC "-//iptel.org//DTD DBSchema V1.0//EN"
  "http://iptel.org/dbschema/dtd/1.0/dbschema.dtd" [

<!ENTITY % entities SYSTEM "entities.xml">
%entities;

]>

<table id="location">
    <name>location</name>
    <version>8</version>

    <column id="location.uid">
	<name>uid</name>
	<type>string</type>
	<size>&id_len;</size>
    </column>

    <column id="location.contact">
	<name>contact</name>
	<type>string</type>
	<size>&uri_len;</size>
    </column>

    <column>
	<name>received</name>
	<type>string</type>
        <null/>
	<size>&uri_len;</size>
    </column>

    <column>
	<name>expires</name>
	<type>datetime</type>
	<default>1970-01-01 00:00:00</default>
    </column>

    <column>
	<name>q</name>
	<type>float</type>
	<default>1.0</default>
    </column>

    <column>
	<name>callid</name>
	<type>string</type>
	<null/>
	<size>&uri_len;</size>
    </column>

    <column>
	<name>cseq</name>
	<type>unsigned int</type>
	<null/>
    </column>

    <column>
	<name>flags</name>
	<type>unsigned int</type>
	<default>0</default>
    </column>

    <column>
	<name>user_agent</name>
	<type>string</type>
	<size>64</size>
	<null/>
    </column>

    <column>
	<name>instance</name>
	<type>string</type>
        <null/>
	<size>&uri_len;</size>
    </column>

    <index>
	<name>location_key</name>
	<unique/>
	<colref linkend="location.uid"/>
	<colref linkend="location.contact"/>
    </index>
    <index>
	<name>location_contact</name>
	<colref linkend="location.contact"/>
    </index>
</table>
